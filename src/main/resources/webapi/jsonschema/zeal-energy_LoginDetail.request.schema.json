{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "LOGIN",
	"description": "Request for Login operation",
	"javaType": "com.energy.zeal.jaxb.LOGIN",
	"type": "object",
	"properties": {
		"userid": {
			"type": "string",
			 "pattern": "^[a-zA-Z0-9]{1,20}$",
			 "description": "User ID details."
				},
				"password": {
					"type": "string",
					"pattern": "^(?=.*[A-Z].*[A-Z])(?=.*[!@#$&*])(?=.*[0-9].*[0-9])(?=.*[a-z].*[a-z].*[a-z]).{8}$",
					"description": "User Password details."
				}
		}
	}
}